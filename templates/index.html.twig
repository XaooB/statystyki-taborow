{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <tr>
            <th>Nazwa</th>
            <th>Kategoria</th>
            <th>Instytucja</th>
        </tr>
        {% for vehicle in vehicles %}
            <tr>
                <td>{{ vehicle.name }}</td>
                <td>{{ vehicle.category }}</td>
                <td>{{ vehicle.institution }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}